	group { name: "elm/gengrid/item/default_grid_new/default";
      data.item: "contents" "elm.swallow.icon elm.swallow.end";
      images {
         image : "org.tizen.setting.png" COMP;
         image : "org.tizen.setting.png" COMP;
         image : "org.tizen.setting.png" COMP;
      }
      parts {
         part { name: "bg";
            type: RECT;
            description { state: "default" 0.0;
			//color: 255 0 0 255;		/* THIS IS BORDER LINE !!! GRID - HORIZENTAL LINE MIDDLE BOTTOM */
			color: 8 8 8 8;		/* THIS IS BORDER LINE !!! GRID - HORIZENTAL LINE MIDDLE BOTTOM */
			//color_class: "W011";
			//color: 255 0 0 255;		/* THIS IS BORDER LINE !!! GRID - HORIZENTAL LINE MIDDLE BOTTOM */
            }
         }
         part { name: "left_top_padding";
            type: RECT;
            scale : 1;
            description { state: "default" 0.0;
			color: 8 8 8 8;
               //min: 1 1;
               min: 0 0;
               align: 0.0 0.0;
               fixed: 1 1;
               rel1.to: "bg";
               rel2 {
                  relative: 0.0 0.0;
                  to: "bg";
               }
            }
         }
         part { name: "right_bottom_padding";
            type: RECT;
            scale: 1;
            description { state: "default" 0.0;
			color: 8 8 8 8;
			//color: 255 8 8 255;
			//color: 255 0 255 255;
               //min: 1 1;
               min: 0 0;
               align: 1.0 1.0;
               visible: 0;
               fixed: 1 1;
               rel1 {
                  relative: 1.0 1.0;
                  to: "bg";
               }
               rel2.to: "bg";
            }
         }
         part { name: "item_bg";
            type: RECT;
            description { state: "default" 0.0;
			//color: 8 8 255 255;
			color_class: "B0217L1";
			//color: 255 0 255 20;
               rel1 {
                  relative: 1.0 1.0;
                  to: "left_top_padding";
               }
               rel2 {
                  relative: 0.0 0.0;
                  to: "right_bottom_padding";
               }
				/*
               image.normal: "org.tizen.setting.png";
               image.border: 1 1 1 1;
				*/
            }
         }
         part { name: "item_bg_left_top_padding";
            type: RECT;
            scale : 1;
            description { state: "default" 0.0;
			color: 8 8 8 8;
			//color: 255 0 255 20;
               //min: 1 1;
               //min: 0 1;
               min: 0 0;
               align: 0.0 0.0;
               visible: 0;
               fixed: 1 1;
               rel1.to: "item_bg";
               rel2 {
                  relative: 0.0 0.0;
                  to: "item_bg";
               }
            }
         }
         part { name: "item_bg_right_bottom_padding";
            type: RECT;
            scale : 1;
            description { state: "default" 0.0;
			color: 8 8 8 8;
               min: 0 1;
               align: 1.0 1.0;
               visible: 0;
               fixed: 1 1;
               rel1 {
                  relative: 1.0 1.0;
                  to: "item_bg";
               }
               rel2.to: "item_bg";
            }
         }
         part {
			name: "item_real_bg";
            type: RECT;
            description { state: "default" 0.0;
			color_class: "B0217L1";
			//color: 0 255 0 255;
               rel1 {
                  relative: 1.0 1.0;
                  to: "item_bg_left_top_padding";
               }
               rel2 {
                  relative: 0.0 0.0;
                  to: "item_bg_right_bottom_padding";
               }
				/*
               image.normal: "org.tizen.setting.png";
				*/
            }
         }
         part { name: "item_real_bg_left_top_padding";
            type: RECT;
            scale : 1;
            description { state: "default" 0.0;
               //min: GENGRID_DEFAULT_ITEM_REAL_BG_RIGHT_PADDING_INC;
               min: 0 0;
               align: 0.0 0.0;
               visible: 0;
               fixed: 1 1;
               rel1.to: "item_real_bg";
               rel2 {
                  relative: 0.0 0.0;
                  to: "item_real_bg";
               }
            }
         }
         part { name: "item_real_bg_right_bottom_padding";
            type: RECT;
            scale : 1;
            description { state: "default" 0.0;
               //min: GENGRID_DEFAULT_ITEM_REAL_BG_RIGHT_PADDING_INC;
               min: 0 0;
               align: 1.0 1.0;
               fixed: 1 1;
               visible: 0;
               rel1 {
                  relative: 1.0 1.0;
                  to: "item_real_bg";
               }
               rel2.to: "item_real_bg";
            }
         }
         part { name: "elm.swallow.icon";
            type: SWALLOW;
            description { state: "default" 0.0;
			color: 8 8 8 8;
			//color_class: "B011";

               rel1 {
                  relative: 1.0 1.0;
                  to: "item_real_bg_left_top_padding";
		  		//offset: 3 0;
		  		offset: 0 1;
               }
               rel2 {
                  relative: 0.0 0.0;
                  to: "item_real_bg_right_bottom_padding";
		  			//offset: -7 0;
		  			offset: 1 -1;
               }
            }
         }
         part { name: "elm.swallow.end";
            type: SWALLOW;
            description { state: "default" 0.0;
			//color_class: "B011";
			color: 8 8 8 8;
               rel1.to: "elm.swallow.icon";
               rel2.to: "elm.swallow.icon";
            }
         }
         part { name: "reorder_bg";
            type: RECT;
            repeat_events: 1;
            description { state: "default" 0.0;
			color: 8 8 8 8;
			//color_class: "B0217";
               rel1.to: "item_bg";
               rel2.to: "item_bg";
            }
            description { state: "enabled" 0.0;
               inherit: "default" 0.0;
               color: 8 8 8 77;
            }
         }
	 SETTING_LAYOUT_FOCUS_PART("bg")
      }
      programs {
         program { name: "go_reorder_disabled";
            signal: "elm,state,reorder,disabled";
            source: "elm";
            action: STATE_SET "default" 0.0;
            target: "reorder_bg";
            target: "focus_bg";
            transition: ACCELERATE 0.3;
         }
         program { name: "go_reorder_enabled";
            signal: "elm,state,reorder,enabled";
            source: "elm";
            action: STATE_SET "enabled" 0.0;
            target: "reorder_bg";
            target: "focus_bg";
            transition: DECELERATE 0.3;
         }
	 SETTING_LAYOUT_FOCUS_PROGRAME
      }
   }

