			part {
				name: "content";
				type: SWALLOW;
				mouse_events: 1;
				scale: 1;
				description {
					state: "default" 0.0;
					align: 0.0 0.0;
					rel1 { relative: 0.0 1.0; offset: 0 0; to: "searchbar_bg"; }
					rel2 { relative: 1.0 1.0; offset: 0 -5; to: "bg"; }
				}
			}

		///////////////////edit mode
			part {
				name: "edit.content";
				type: SWALLOW;
				mouse_events: 1;
				scale: 1;
				description {
					state: "default" 0.0;
					align: 0.0 0.0;
					rel1 { relative: 0.0 0.0; offset: 0 30; to: "content"; }
					rel2 { relative: 1.0 1.0; offset: 0 0; to: "content"; }
				}
			}
			part {
				name: "edit.separator";
				type: IMAGE;
				scale: 1;
				description {
					state: "default" 0.0;
					align: 0.0 0.0;
					visible: 1;
					rel1 { relative: 0.0 0.0; offset: 0 0; to: "bg"; }
					rel2 { relative: 1.0 0.0; offset: 0 2; to: "edit.content"; }
					image {
						normal: "org.tizen.setting.png";
					}
				}
			}
			part { name: "edit.content.left.separator1"; //to cover the gengrid top border line
			    type: IMAGE;
			    scale: 1;
			    description { state: "default" 0.0;
			       align: 0.0 0.0;
			       rel1 { relative: 0.0 0.331; offset: 0 0;}
			       rel2 { relative: 0.0 0.331; offset: 16 1;}
			       image {
				       normal: "org.tizen.setting.png";
			       }
			    }
			    description {
				    state: "hide" 0.0;
				    inherit: "default" 0.0;
				    visible: 0;
			    }
			 }
			part { name: "edit.content.right.separator1"; //to cover the gengrid top border line
			    type: IMAGE;
			    scale: 1;
			    description { state: "default" 0.0;
			       align: 0.0 0.0;
			       rel1 { relative: 1.0 0.331; offset: -16 0;}
			       rel2 { relative: 1.0 0.331; offset: 0 1;}
			       image {
				       normal: "org.tizen.setting.png";
			       }
			    }
			    description {
				    state: "hide" 0.0;
				    inherit: "default" 0.0;
				    visible: 0;
			    }
			 }

			part {
				name: "edit.textblock";
				type: TEXTBLOCK;
				scale: 1;
				description {
					min: 0 45;
					fixed: 0 1;
					state: "default" 0.0;
					align: 0.5 0.5;
					rel1 { relative: 0.0 0.6; offset: 0 0; to: "bg"; }
					rel2 { relative: 1.0 0.8; offset: 0 0; to: "bg"; }
					text {
						//align: 0.5 0.5;
						min: 0 1;
						//font: "Tizen:style=Bold";
						//size: 30;
						//text_class:"tizen";
						style: textblock_style_center_45;
					}
				}
			}

		} // end of parts
	}
}
